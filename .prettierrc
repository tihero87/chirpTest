{
  "tabWidth": 2,
  "printWidth": 120,
  "useTabs": false,
  "semi": true,
  "singleQuote": true,
  "quoteProps": "as-needed",
  "jsxSingleQuote": true,
  "trailingComma": "all",
  "bracketSpacing": true,
  "arrowParens": "avoid",
  "plugins": [
    "@trivago/prettier-plugin-sort-imports"
  ],
  "importOrder": [
    "^(react|react-router-dom|react-intersection-observer)$",
    "^(@tanstack)$",
    "^(@mui/material)$",
    "<THIRD_PARTY_MODULES>",
    "@/src/(.*)$",
    "@/assets/(.*)$",
    "@/app/(.*)$",
    "@/pages/(.*)$",
    "@/widgets/(.*)$",
    "@/entities/(.*)$",
    "@/features/(.*)$",
    "@/shared/(.*)$",
    "@/hooks/(.*)$",
    "^[./]"
  ],
  "importOrderSeparation": true,
  "importOrderSortSpecifiers": true
}
